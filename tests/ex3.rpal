let rec f(a)= a eq 1 -> 1 |
                        a le 0 -> 0 | 
                                  f(a-1) + f(a-2) 
in let rec range (a,  b) = a gr b -> 'Error' | 
                                  a eq b -> (nil) | 
                                            (range (a, b-1)) aug f(b)
in let Fib_rang(a,b) = Print(range (5 ,13))
in Fib_rang(5,13)
